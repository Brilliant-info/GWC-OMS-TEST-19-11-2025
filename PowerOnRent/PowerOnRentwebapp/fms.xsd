                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        CustomerFirstName + ' ' + CustomerLastName AS Fullname,  convert(varchar,DateofBirth,103)DateofBirth,   convert(varchar,IDExpiryDate,103)IDExpiryDate, Email, POBox, KahramaaID, AltContactNumber, PreferredLanguage, InstillationAddress,
 convert(varchar,OrderCreationDate,103) OrderCreationDate, VodafoneStoreName, 
                         OrderID, Nationality,
						 
case when buildingname=' ' then buildingname+' '+ streetname +' '+ city +' '+country when buildingname&lt;&gt;' ' then  buildingname+', '+ streetname +', '+ city +', '+country end as address1,
						 Idt,idtype,country,

case when InstBuildingName=' ' then InstBuildingName+' '+InstStreetname+' '+ InstCity+' '+ InstCountry+' '+ InstPOBox+' '+ InstArea
when InstBuildingName&lt;&gt;' ' then  InstBuildingName+', '+InstStreetname+', '+ InstCity+', '+ InstCountry+', '+ InstPOBox+', '+ InstArea end
						 as instaddress,
						  InstZone, DelvArea, DelvZone, CustomerType, IssuingCountry, VisaNumber, AppointmentId, 
                         AppointmentDateTime, HomePOBox, HomeArea, HomeZone,

						 'Appointmnet ID:'+AppointmentId +' '+'Appointment Date:'+ CONVERT(VARCHAR(20),AppointmentDateTime, 106) as Apointdetails




FROM            dbo.tECommHead</CommandText>
